  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "from team_data import *\n",
    "A1 = coo_matrix((train[:, 2], (range(m), train[:, 0])), shape=(m, n)).toarray()\n",
    "A2 = coo_matrix((-train[:, 2], (range(m), train[:, 1])), shape=(m, n)).toarray()\n",
    "A = A1 + A2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = cp.Variable(n)\n",
    "objective = cp.Minimize(cp.sum(cp.logistic(-2 / sigma * A @ a)))\n",
    "constraint = [a <= 1, a >= 0]\n",
    "prob = cp.Problem(objective, constraint)\n",
    "prob.solve()\n",
    "a_hat = a.value - min(a.value)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
