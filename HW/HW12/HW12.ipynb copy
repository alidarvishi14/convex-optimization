   "source": [
    "prediction = np.zeros(shape=(m, 3))\n",
    "i = 0\n",
    "for j in range(n):\n",
    "    for k in range(j + 1, n):\n",
    "        prediction[i, :] = [j, k, np.sign(a_hat[j] - a_hat[k])]\n",
    "        i = i + 1\n",
    "print(\n",
    "    \" prediction accuracy:\",\n",
    "    round(100 * sum(prediction[:, 2] == test[:, 2]) / m, 2),\n",
    "    \"%\",\n",
    "    \"\\n biased prediction accuracy:\",\n",
    "    100 * round(sum(train[:, 2] == test[:, 2]) / m, 2),\n",
    "    \"%\",\n",
    ")"
